<?php

function juanrueda_response_init() {
  $path = drupal_get_path('module', 'juanrueda_response');
  drupal_add_js($path . '/juanrueda_response.js');


  if(strpos($_SERVER['HTTP_USER_AGENT'], 'MSIE 7.0') !== FALSE) {

    $js = "jQuery(document).ready(function () { jQuery('body').addClass('responsive-layout-normal'); alert('hola'); });";
    drupal_add_js($js, array('type' => 'inline', 'scope' => 'footer', 'weight' => 5));
  }


}

